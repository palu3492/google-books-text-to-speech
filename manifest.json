{
  "author": "Alex Palumbo",
  "manifest_version": 2,
  "name": "Google Books Text to Speech",
  "description": "Reads Google Books audibly",
  "version": "0.0.1",
  "permissions": [
    "ttsEngine",
    "tts"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://books.googleusercontent.com/books/reader/display?guctk=*"
      ],
      "run_at": "document_end",
      "js": ["js/jquery-3.4.1.min.js", "js/content.js"],
      "css": ["css/style.css"],
      "all_frames": true
    }
  ]
}